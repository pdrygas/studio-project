{
  "manifest_version": 2,
  "name": "Content collector",
  "description": "TODO",
  "version": "1.0",
  "browser_action": {
    "default_icon": "resources/icon.png",
    "default_popup": "login.html"
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["content.js"]
    }
  ]
}